$('.dental-plan-options').html("<%= escape_javascript(render partial: 'dental_plan_options', locals: {plan_year_id: @plan_year_id, location_id: @location_id}) %>");

start_on = $("#plan_year_start_on").val().substr(0,4);
$('.dental .plan-options a').each(function() {
  var url = $(this).attr('href');
  $(this).attr('href', url+"&start_on="+start_on);
});
$("#plan_year_start_on").on('change', function() {
  start_on = $(this).val().substr(0,4);
  $('.dental .plan-options a').each(function() {
    var url = $(this).attr('href');
    $(this).attr('href', url+"&start_on="+start_on);
  });

});
